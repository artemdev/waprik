<% provide(:title, @film.title) %>

<%= error_messages_for @film %>

<%= form_for(:film, url: {action: 'create', movie_title: @movie.title}, method: :post, multipart: true) do |f| %>

	<%= render partial: 'admin/shared/film_form', locals: {:f => f, :film => @film} %>

	<%= f.submit "Добавить фильм"%>
<% end %>